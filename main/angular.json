{
    "$schema": "../node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "projects":
    {
        "app":
        {
            "root": "",
            "sourceRoot": "",
            "projectType": "application",
            "architect":
            {
                "build": 
                {
					"builder": "@angular-architects/native-federation:build",
					"options": 
                    {
					},
					"configurations": 
                    {
						"production": 
                        {
							"target": "app:esbuild:production"
						},
						"development": 
                        {
							"target": "app:esbuild:development",
							"dev": true
						}
					},
					"defaultConfiguration": "development"
				},
				"serve": 
                {
					"builder": "@angular-architects/native-federation:build",
					"options": 
                    {
						"target": "app:serve-original:development",
						"rebuildDelay": 500,
						"dev": true,
                        "port": 0,
                        "cacheExternalArtifacts": true
					}
				},
                "esbuild":
                {
                    "builder": "@angular-devkit/build-angular:application",
                    "options":
                    {
                        "baseHref": "/",
                        "outputPath":
                        {
                            "base": "wwwroot",
                            "browser": "browser",
                            "server": "server"
                        },
                        "index": "index.html",
                        "browser": "app/main.browser.ts",
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "polyfills":
                        [
                            "es-module-shims",
                            "modernizr"
                        ],
                        "assets": 
                        [
                            {
                                "glob": "**/*",
                                "input": "../node_modules/ngx-scrollbar/assets/",
                                "output": "assets"
                            }
                        ],
                        "styles":
                        [
                            "content/site.scss",
                            "content/dochadzka.scss"
                        ],
                        "allowedCommonJsDependencies":
                        [
                            "konami",
                            "mousetrap",
                            "extend",
                            "sourcemapped-stacktrace",
                            "store",
                            "node-localstorage",
                            "store/plugins/expire",
                            "store/storages/sessionStorage"
                        ],
                        "scripts":
                        [
                            {
                                "input": "config/configBrowserOverride.js",
                                "bundleName": "configBrowserOverride"
                            },
                            "konami"
                        ],
                        "serviceWorker": false,
                        "loader": 
                        {
                            ".monaco-type": "text"
                        },
                        "security": 
                        {
                            "autoCsp": false
                        }
                    },
                    "configurations":
                    {
                        "production":
                        {
                            "define":
                            {
                                "isProduction": "true",
                                "jsDevMode": "false",
                                "ngDesignerMetadata": "true",
                                "ngRelationsDebugger": "false"
                            },
                            "budgets":
                            [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "4mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development":
                        {
                            "define":
                            {
                                "isProduction": "false",
                                "jsDevMode": "true",
                                "ngDesignerMetadata": "true",
                                "ngRelationsDebugger": "false"
                            },
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "serve-original":
                {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options":
                    {
                        "hmr": true,
                        "proxyConfig": "proxy.conf.json",
                        "port": 8888
                    },
                    "configurations":
                    {
                        "production":
                        {
                            "buildTarget": "app:esbuild:production"
                        },
                        "development":
                        {
                            "buildTarget": "app:esbuild:development"
                        }
                    },
                    "defaultConfiguration": "development"
                }
            }
        }
    },
    "cli":
    {
        "cache":
        {
            "environment": "local",
            "enabled": true
        },
        "analytics": false,
        "warnings":
        {
            "versionMismatch": true
        }
    }
}