{
  "$schema": "../node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": 
  [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": 
      {
        "files": 
        [
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/media/*.*"
        ]
      }
    },
    {
      "name": "markdowns",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": 
      {
        "files": 
        [
          "/md/**"
        ]
      }
    }
  ]
}
